{
    "name": "alexclark-nz/boltmail",
    "description": "A package for interacting with the Boltmail API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Clark",
            "email": "hello@alexclark.co.nz",
            "homepage": "https://alexclark.co.nz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12"
    },
    "autoload": {
        "psr-4": {
            "AlexClark\\Boltmail\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexClark\\Boltmail\\BoltmailServiceProvider"
            ],
            "aliases": {
                "Boltmail": "AlexClark\\Boltmail\\Facades\\Boltmail"
            }
        }
    }
}
